<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="OBSNotifier.Forms.AboutWindow"
        Title="AboutWindow">
	<StackPanel>
		<Label Name="labelProductName"/>
		<Label Name="labelVersion"/>
		<Label Name="labelCopyright"/>
		<Label Name="labelCompanyName"/>
		<Label Name="textBoxDescription"/>
		<Label Name="link_source_code" PointerPressed="Label_PointerPressed"/>
	</StackPanel>
</Window>
