<?xml version="1.0"?>
<ResourceDictionary xml:space="preserve" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!-- Use &#10; instead of \n -->
    
    <!-- Default Module -->
    <v:String x:Key="module_default_name">Stack of notifications</v:String>
    <v:String x:Key="module_default_desc">This is the default notification module.&#10;It displays one or more notifications located in the corner of the monitor.</v:String>
    <v:String x:Key="module_default_setting_category_blockss">Blocks</v:String>

    <!-- Nvidia-Like Module -->
    <v:String x:Key="module_nvidia_like_name">Nvidia-Like</v:String>
    <v:String x:Key="module_nvidia_like_desc">Simple notification similar to Nvidia's notifications</v:String>
    
    <!-- Settings -->
    <!-- TODO "Прозрачное для кликов" -->
    <v:String x:Key="module_common_setting_clickthrough_hint">'Click Through' will only work when notification interaction modes such as 'Quick Actions' are disabled.</v:String>
    
    <!-- Settings Categories-->
    <v:String x:Key="module_common_setting_category_icon">Icon</v:String>
    <v:String x:Key="module_common_setting_category_colors">Colors</v:String>
    <v:String x:Key="module_common_setting_category_animation">Animation</v:String>
    <v:String x:Key="module_common_setting_category_quick_actions">Quick Actions</v:String>
    
</ResourceDictionary>